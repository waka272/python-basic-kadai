        "  def __init__(self,name,age):\n",
        "    # 属性を定義\n",
        "    self.name=name\n",
        "    self.age=age\n",
        "\n",
        "  # メソッドを定義する\n",
        "  def printinfo(self):\n",
        "    print(self.name,self.age)\n",
        "\n",
        "# インスタンス作成\n",
        "person=Human(\"さむらい\",56)\n",
        "\n",
        "person.printinfo()"
